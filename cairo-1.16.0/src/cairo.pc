prefix=/home/jianglei/svg/build/usr/local
exec_prefix=${prefix}
libdir=${exec_prefix}/lib
includedir=${prefix}/include

Name: cairo
Description: Multi-platform 2D graphics library
Version: 1.16.0

Requires.private:   gobject-2.0 glib-2.0 >= 2.14      pixman-1 >= 0.30.0    fontconfig >= 2.2.95 freetype2 >= 9.7.3    glesv2 libpng 
Libs: -L${libdir} -lcairo
Libs.private:           -lz -lz   -lz -lEGL  -lGLESv2  
Cflags: -I${includedir}/cairo
